{
	"name": "thia",
	"version": "0.1.0",
	"description": "AutoML using your own hardware",
	"main": "./dist/main.bundle.js",
	"scripts": {
		"start": "electron --trace-warnings ./dist/main.bundle.js",
		"dev:app": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color --stats-error-details",
		"dev": "concurrently --kill-others \"npm run minio\" \"npm run dev:app\"",
		"minio": "minio server minio/minio-data",
		"prod": "rimraf dist && cross-env NODE_ENV=production webpack --color",
		"test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
		"build:app": "npm run prod && rimraf out && electron-builder build --win --x64 --publish never",
		"build:app-unpacked": "npm run prod && rimraf out && electron-builder build --win --x64 --dir --publish never",
		"build": "npm run build:py && npm run build:win",
		"build:unpacked": "npm run build:py && npm run build:win-unpacked",
		"build:engine": "rimraf build-py && rimraf extraResources/engine && pyinstaller --workpath=./build-py --distpath=./extraResources engine.spec",
		"publish": "rimraf out && electron-builder --win --x64 --publish always",
		"publish:dev": "rimraf out && electron-builder --win --x64  --publish always --config.win.publish.endpoint=http://localhost:9000 --config.win.publish.bucket=test-bucket",
		"tests": "electron-publish",
		"lint": "echo Lint-Check Main && npm run lint-main && echo Lint-Check Renderer && npm run lint-renderer",
		"lint-renderer": "eslint src/renderer/** -c=\"renderer.eslintrc.json\"",
		"lint-main": "eslint src/main/** -c=\"main.eslintrc.json\"",
		"lint-fix-renderer": "eslint src/renderer/** -c=\"renderer.eslintrc.json\" --fix",
		"lint-fix-main": "eslint src/main/** -c=\"main.eslintrc.json\" --fix",
		"lint-fix-dry-renderer": "eslint src/renderer/** -c=\"renderer.eslintrc.json\" --fix-dry-run",
		"lint-fix-dry-main": "eslint src/main/** -c=\"main.eslintrc.json\" --fix-dry-run",
		"deploy:functions-prod": "firebase deploy --project thia-1458b --only functions",
		"deploy:functions-dev": "firebase deploy --project thia-dev --only functions",
		"postinstall": "patch-package"
	},
	"build": {
		"appId": "Thia",
		"productName": "Thia",
		"copyright": "Copyright Â© 2021 Thia",
		"directories": {
			"app": "./dist/",
			"output": "./out/"
		},
		"win": {
			"target": "nsis",
			"asar": true,
			"requestedExecutionLevel": "requireAdministrator",
			"publish": {
				"provider": "s3",
				"bucket": "thia-app-downloads"
			}
		},
		"nsis": {
			"oneClick": false,
			"perMachine": true,
			"allowToChangeInstallationDirectory": true,
			"deleteAppDataOnUninstall": true,
			"license": "LICENSE",
			"runAfterFinish": true,
			"installerIcon": "build/icon.ico",
			"uninstallerIcon": "build/icon.ico"
		},
		"buildVersion": "1",
		"extraResources": [
			"./extraResources/**"
		],
		"afterPack": "./electron-builder/hooks/afterPack.js"
	},
	"author": "Thia AI",
	"license": "SEE LICENSE IN LICENSE",
	"dependencies": {
		"@chakra-ui/icons": "^1.0.13",
